# $Id: graph.axiom.n3,v 1.13 2004/06/23 13:15:20 amdus Exp $

@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix : <http://www.agfa.com/w3c/euler/graph.axiom#>.

:path = :route.
:parijs = :paris.

:paris :twoway :orleans.
:parijs :twoway :chartres.
:paris :twoway :amiens.
:orleans :twoway :blois.
:orleans :twoway :bourges.
:blois :twoway :tours.
:chartres :twoway :lemans.
:lemans :twoway :angers.
:lemans :twoway :tours.
:angers :oneway :nantes.

:oneway rdfs:subPropertyOf :path.
:twoway rdfs:subPropertyOf :oneway, :path; a owl:SymmetricProperty.
:path a owl:TransitiveProperty.

