<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:pol="urn:govshare.info/rdf/politico/"
	xmlns:bill="urn:govshare.info/rdf/usbill/"
	
	xmlns:query="http://purl.oclc.org/NET/rsquary/">

	<rdfs:Class rdf:about="http://purl.oclc.org/NET/rsquary/Query">
		<rdfs:label>RSquary Query</rdfs:label>
	</rdfs:Class>
	
	<rdfs:Class rdf:about="http://purl.oclc.org/NET/rsquary/Variable">
		<rdfs:label>RSquary Variable</rdfs:label>
	</rdfs:Class>

	<rdf:Property rdf:about="http://purl.oclc.org/NET/rsquary/select">
		<rdfs:label>Select</rdfs:label>
		<rdfs:domain rdf:resource="http://purl.oclc.org/NET/rsquary/Query"/>
		<rdfs:range rdf:resource="http://purl.oclc.org/NET/rsquary/Variable"/>
	</rdf:Property>

	<query:Query rdf:ID="query">
		<query:select rdf:resource="x"/>
		<query:select rdf:resource="y"/>
	</query:Query>
	
	<pol:Politician rdf:ID="x">
		<foaf:name rdf:resource="y"/>
	</pol:Politician>

	<bill:Bill rdf:ID="y">
		<bill:sponsor rdf:resource="x"/>
	</bill:Bill>

	<!--<rdf:Statement rdf:ID="thisid">
