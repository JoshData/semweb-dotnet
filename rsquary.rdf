<?xml version="1.0"?>

<!-- http://taubz.for.net/code/semweb/rsquary-0/rsquary.rdf -->
<!-- http://purl.oclc.org/NET/rsquary -->

<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	
	xmlns:rsquary="http://purl.oclc.org/NET/rsquary/">

	<rdfs:Class rdf:about="http://purl.oclc.org/NET/rsquary/Query">
		<rdfs:label>RSquary Query</rdfs:label>
		<dc:description>A Query is the start of a graph that is to be matched against a target RDF model.</dc:description>
	</rdfs:Class>
	
	<rdfs:Class rdf:about="http://purl.oclc.org/NET/rsquary/Variable">
		<rdfs:label>RSquary Variable</rdfs:label>
		<dc:description>The Variable class is a supertype of all nodes in the query graph that are to be matched with nodes in the target RDF model, including literals.</dc:description>
	</rdfs:Class>

	<rdf:Property rdf:about="http://purl.oclc.org/NET/rsquary/select">
		<rdfs:label>Select</rdfs:label>
		<rdfs:domain rdf:resource="http://purl.oclc.org/NET/rsquary/Query"/>
		<rdfs:range rdf:resource="http://purl.oclc.org/NET/rsquary/Variable"/>
		<dc:description>The select property indicates the resource designated as the object of this property is a Variable in the query graph which is to be matched up with a node in the target RDF model.</dc:description>
	</rdf:Property>

</rdf:RDF>

